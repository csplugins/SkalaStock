Welcome to SkalaStock!
<br />
<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:q, "Market cap:") %>
  <%= text_field_tag(:q, nil, placeholder: "1,000,000,000") %>
  <br />
  <%= label_tag(:r, "PE Ratio:") %>
  <%= text_field_tag(:r, nil, placeholder: "20") %>
  <br />
  <%= label_tag(:s, "Dividend:") %>
  <%= text_field_tag(:s, nil, placeholder: "2.5") %>
  <br />
  <%= submit_tag("Search") %>
<% end %>
